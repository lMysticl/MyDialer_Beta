                         android:allowUndo="false"
                         android:focusableInTouchMode="false" android:focusable="false"
                         android:nestedScrollingEnabled="false"
                         android:fontFamily="@string/abc_font_family_display_2_material"
                         android:src="@drawable/left_arrow"
                         android:baselineAlignBottom="false"
                         android:clickable="true" android:contextClickable="false" android:cropToPadding="false"
                         android:adjustViewBounds="false" android:visibility="visible"
                         android:outlineProvider="bounds" android:translationZ="0dp"
