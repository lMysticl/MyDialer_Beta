    <TextView
            android:layout_width="wrap_content"
            android:layout_height="149dp"
            android:id="@+id/textView2" android:layout_gravity="center_horizontal|top"
            android:textColor="@color/bright_foreground_material_light"
            android:textSize="50sp"
            android:textIsSelectable="true"
            android:background="@color/foreground_material_dark"
            android:gravity="bottom|end" android:contextClickable="false"
            android:autoText="false" android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true" android:layout_above="@+id/relativeLayout"/>

