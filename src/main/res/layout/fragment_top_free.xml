                         android:focusableInTouchMode="false"
                         android:focusable="false" android:nestedScrollingEnabled="false"
                         android:src="@android:drawable/sym_action_call"
                         android:baselineAlignBottom="false" android:clickable="true" android:contextClickable="false"
                         android:cropToPadding="false"
                         android:adjustViewBounds="false" android:visibility="visible"
                         android:outlineProvider="bounds"
                         android:translationZ="0dp"
                         android:backgroundTint="@color/material_blue_grey_800"
                         android:contentDescription="@string/_13" android:background="@drawable/oval_button"
                         android:layout_marginLeft="50dp"/>
            <ImageButton android:layout_width="55sp" android:layout_height="55dp" android:text="@string/lt"
                         android:id="@+id/button14"
                         android:textSize="35sp"
                         android:textColor="@color/primary_material_light"
                         android:padding="0dp" android:breakStrategy="high_quality"
