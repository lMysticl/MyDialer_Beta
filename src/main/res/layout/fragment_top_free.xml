            <Button android:layout_width="55sp" android:layout_height="55dp" android:text="@string/_0"
                    android:id="@+id/button9"
                    android:textSize="35sp"
                    android:textColor="@color/switch_thumb_normal_material_light"
                    android:padding="0dp"
                    android:breakStrategy="high_quality"
                    android:elegantTextHeight="false" android:longClickable="false"
                    android:allowUndo="false"
                    android:focusableInTouchMode="false" android:focusable="false"
                    android:nestedScrollingEnabled="false"
