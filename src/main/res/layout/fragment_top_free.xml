                         android:src="@android:drawable/sym_action_chat" android:clickable="true"
                         android:baselineAlignBottom="true" android:cropToPadding="false"
                         tools:targetApi="m" android:adjustViewBounds="false"
                         android:importantForAccessibility="auto"
                         android:backgroundTint="@color/material_blue_grey_800" android:padding="0dp"
                         android:translationZ="0dp"
                         android:contentDescription="@string/_14" android:background="@drawable/oval_button"
                         android:layout_marginLeft="50dp"/>
            <ImageButton android:layout_width="55sp" android:layout_height="55dp" android:text="@string/call"
                         android:id="@+id/button15"
                         android:textSize="35sp"
                         android:textColor="@color/primary_material_light"
                         android:padding="0dp"
                         android:breakStrategy="high_quality" android:elegantTextHeight="false"
