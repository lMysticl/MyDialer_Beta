                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true" android:layout_alignParentStart="true">
            <ImageButton android:layout_width="55sp" android:layout_height="55dp" android:text="@string/sms"
                         android:id="@+id/button13"
                         android:textSize="35sp"
                         android:contextClickable="false"
                         android:textColor="@color/primary_material_light"
                         android:breakStrategy="high_quality"
                         android:elegantTextHeight="false" android:longClickable="false"
